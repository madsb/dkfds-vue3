    "build:core": "pnpm --filter dkfds-vue3-core build",
    "build:extra": "pnpm --filter dkfds-vue3-extra build",
    "build:utils": "pnpm --filter dkfds-vue3-utils build",
    "build:lib": "pnpm --filter dkfds-vue3 build",
    "build:demo": "pnpm --filter example build",
    "preview": "pnpm --filter example preview",
    "type-check": "pnpm -r run type-check",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "npx eslint packages/** examples/**",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier packages/** examples/** --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "clean": "pnpm -r run clean",
    "init": "pnpm install && pnpm run build"
