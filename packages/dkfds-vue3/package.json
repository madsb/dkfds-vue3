{
  "name": "dkfds-vue3",
  "version": "0.5.0",
  "module": "./core/index.mjs",
  "types": "core/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitewillow/dkfds-vue3"
  },
  "exports": {
    ".": {
      "import": "./core/index.mjs",
      "types": "./core/index.d.ts"
    },
    "./extra": {
      "import": "./extra/index.mjs",
      "types": "./extra/index.d.ts"
    },
    "./utils": {
      "import": "./utils/index.mjs",
      "types": "./utils/index.d.ts"
    },
    "./core/assets/*": "./core/src/assets/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "node build.js",
    "typings": "vue-tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist"
  },
  "dependencies": {
    "dkfds": "^11.0.0",
    "dkfds-vue3-core": "workspace:*",
    "dkfds-vue3-extra": "workspace:*",
    "dkfds-vue3-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "sass": "^1.56.1",
    "typescript": "^5.8.3",
    "vite": "^3.2.5",
    "vue": "^3.2.44",
    "vue-tsc": "^2.2.12"
  }
}